{
  "name": "RMS",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "remoteUser": "node",
  "workspaceFolder": "/workspaces/rms",
  "postCreateCommand": "pnpm install && cd packages/api && prisma generate && prisma db push",
  "customizations": {
    "vscode": {
      "extensions": [
        "formulahendry.auto-close-tag",
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "Prisma.prisma"
      ]
    }
  }
}
